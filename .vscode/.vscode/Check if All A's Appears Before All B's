class Solution {
    public boolean checkString(String s) {
            int temp=0;
        for (int i=0;i<s.length();i++){
                if(temp==1){
                        if(s.charAt(i)=='a')
                                return false;
                }
                if(s.charAt(i)=='b')
                        temp=1;
        }
            return true;
    }
}
