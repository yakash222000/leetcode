class Solution {
    public boolean squareIsWhite(String coordinates) {
        String str="aceg";
        String s=coordinates.substring(0,1);
        int a = coordinates.charAt(1) - '0';
            if(str.contains(s)&& a%2!=0)
                    return false;
            if(!(str.contains(s))&& a%2==0)
                    return false;
            return true;
    }
}
